public class Main
{
	public static void main(String[] args) {
	    outer:
	    for(int i=1;i<=5;i++){
	        inner:
	        for(int j=1;j<=5;j++){
	            if(i==2 && j<5)
	            continue ;
	               if(i==4 && j<5)
	               continue;
	               if(i==3 && j<3)
	               continue;
	         
	        System.out.print("*"); 
	        }
	      System.out.println(" ");  
	    }
		
	}
}
