
import java.util.*;
class Main {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        int [] a = new int[5];
        int t=1;
        int count=0;
        // for input
        for(int i=0;i<a.length;i++){
            a[i] = sc.nextInt();
        }
        
        // for output
        for(int i=0;i<a.length;i++){
            if(a[i] == t)
                 count += 1;
            
        }
        System.out.print(count);
    }
}
